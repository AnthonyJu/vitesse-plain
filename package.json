{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip -d",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^12.7.0",
    "axios": "^1.8.1",
    "element-plus": "^2.9.5",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.4.0",
    "@anthony-ju/eslint-config": "^0.2.9",
    "@anthony-ju/stylelint-config": "^0.4.0",
    "@iconify-json/carbon": "^1.2.8",
    "@types/node": "^22.13.9",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/runtime-dom": "^3.5.13",
    "beasties": "^0.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.4.3",
    "pick-zip": "^0.2.0",
    "pnpm": "^10.5.2",
    "sass": "^1.85.1",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.15.0",
    "taze": "^18.6.0",
    "typescript": "^5.8.2",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^6.2.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-support-setup-name": "^0.3.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "*.{vue,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}

{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build:zip": "pnpm run build && pnpm run pack:zip",
    "pack:zip": "rimraf dist.zip && jszip add dist/* -o ./dist.zip",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.3",
    "element-plus": "^2.4.4",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@anthony-ju/eslint-config": "^0.2.4",
    "@anthony-ju/stylelint-config": "^0.2.8",
    "@types/node": "^20.10.6",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.2",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "https-localhost": "^4.7.1",
    "jszip-cli": "^1.4.24",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.1.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.10",
    "vite-bundle-visualizer": "^1.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-vue-devtools": "^7.0.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.js,ts,vue": "eslint --fix"
  }
}

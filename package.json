{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,ts,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "element-plus": "^2.5.5",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@anthony-ju/eslint-config": "^0.2.5",
    "@anthony-ju/pick-zip": "^0.1.0",
    "@anthony-ju/stylelint-config": "^0.2.8",
    "@types/node": "^20.11.16",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "https-localhost": "^4.7.1",
    "javascript-obfuscator": "^4.1.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.70.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.2.1",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.12",
    "vite-bundle-visualizer": "^1.1.0-alpha.1",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-vue-devtools": "^7.0.14",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.js,ts,vue": "eslint --fix"
  }
}

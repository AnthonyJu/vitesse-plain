    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@anthony-ju/eslint-config": "^0.1.2",
    "@anthony-ju/stylelint-config": "^0.1.1",
    "@types/node": "^20.3.1",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.53.1",
    "@vitejs/plugin-vue": "^4.2.3",

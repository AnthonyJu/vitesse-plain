{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip -d",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^13.5.0",
    "axios": "^1.10.0",
    "element-plus": "^2.10.4",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@anthony-ju/eslint-config": "^0.2.9",
    "@anthony-ju/stylelint-config": "^0.4.0",
    "@iconify-json/carbon": "^1.2.11",
    "@types/node": "^22.16.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/runtime-dom": "^3.5.17",
    "beasties": "^0.3.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^16.1.2",
    "pick-zip": "^0.2.0",
    "pnpm": "^10.13.1",
    "sass": "^1.89.2",
    "simple-git-hooks": "^2.13.0",
    "stylelint": "^16.22.0",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^7.0.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-support-setup-name": "^0.3.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "*.{vue,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}

{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "element-plus": "^2.8.4",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.5",
    "pinia-plugin-persistedstate": "^4.1.2",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@anthony-ju/eslint-config": "^0.2.8",
    "@anthony-ju/stylelint-config": "^0.4.0",
    "@iconify-json/carbon": "^1.2.4",
    "@types/node": "^22.8.7",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.63.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/runtime-dom": "^3.5.12",
    "critters": "^0.0.25",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.10",
    "pick-zip": "^0.1.0",
    "pnpm": "^9.12.3",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.10.0",
    "taze": "^0.18.0",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.10",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-vue-devtools": "^7.6.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-support-setup-name": "^0.3.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "*.{vue,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}

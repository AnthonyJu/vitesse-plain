{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"src/**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vite build",
    "build-zip": "vite build && pick-zip",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "element-plus": "^2.7.7",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.3",
    "@anthony-ju/eslint-config": "^0.2.5",
    "@anthony-ju/stylelint-config": "^0.2.10",
    "@iconify-json/carbon": "^1.1.36",
    "@types/node": "^20.14.10",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.61.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/runtime-dom": "^3.4.31",
    "critters": "^0.0.24",
    "cross-env": "^7.0.3",
    "eslint": "^9.7.0",
    "eslint-plugin-format": "^0.1.2",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.7",
    "pick-zip": "^0.1.0",
    "pnpm": "^9.5.0",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.7.0",
    "taze": "^0.15.1",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-support-setup-name": "^0.3.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.0.26"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{vue,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  }
}

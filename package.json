{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "lint:style": "stylelint **/*.{css,scss,vue} --fix",
    "build": "vite build",
    "pack:zip": "rimraf dist.zip && jszip add dist/* -o ./dist.zip",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.0",
    "element-plus": "^2.4.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@anthony-ju/eslint-config": "^0.2.2",
    "@anthony-ju/stylelint-config": "^0.2.2",
    "@types/node": "^20.8.10",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "https-localhost": "^4.7.1",
    "jszip-cli": "^1.4.24",
    "lint-staged": "^15.0.2",
    "pnpm": "^8.10.2",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "taze": "^0.12.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.5.0",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-webfont-dl": "^3.8.1",
    "vue-tsc": "^1.8.22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

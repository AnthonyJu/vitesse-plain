  "devDependencies": {
    "@anthony-ju/eslint-config": "^0.1.0",
    "@anthony-ju/stylelint-config": "^0.0.2",
    "@types/node": "^20.1.2",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue-macros/volar": "^0.9.8",
